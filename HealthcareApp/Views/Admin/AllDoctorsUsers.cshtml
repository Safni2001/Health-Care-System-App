@model IEnumerable<HealthcareApp.Models.ApplicationUser>
@{
    ViewData["Title"] = "Doctor Users";
}
<h2>Doctor Users</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var d in Model)
    {
        <tr>
            <td>@(d.FullName ?? d.UserName)</td>
            <td>@d.Email</td>
        </tr>
    }
    </tbody>
</table>
